{
  "expo": {
    "name": "pokedex-rn",
    "slug": "pokedex-rn",
    "version": "1.0.0",
    "scheme": "pokedex-rn",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchModeExperimental": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gilsouza.pokedexrn"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gilsouza.pokedexrn"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e4dfae12-0917-4bba-b6ba-234155cdae6f"
      }
    }
  }
}
